<% if @membership.valid? %>
  jQuery('#tab-content-memberships').html('<%= escape_javascript render(:partial => 'memberships') %>');
  jQuery('#member-<%= @membership.id %>').effect("highlight", {}, 2000);
<% else %>
  alert('<%= escape_javascript(l(:notice_failed_to_save_members, :errors => @membership.errors.full_messages.join(', '))) %>');
<% end %>
